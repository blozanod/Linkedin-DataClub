neer'\n","jobs_shown = 10\n","\n","df = getting_data(keywords, jobs_shown=jobs_shown)\n","engine = create_engine('sqlite:///jobs.db')\n","df.to_sql('remoteokjobs', engine, if_exists='replace', index=False)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"moRv6WDBmnqF","executionInfo":{"status":"ok","timestamp":1763339679886,"user_tz":300,"elapsed":674,"user":{"displayName":"Alyssa Sirichoktanasup","userId":"09128764782327088195"}},"outputId":"5a74b84c-ec22-4d70-cae4-aa65aacc8432"},"execution_count":12,"outputs":[{"output_type":"execute_result","data":{"text/plain":["99"]},"metadata":{},"execution_count":12}]}]}